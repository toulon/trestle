extends <%=model.relativeRootPath%>/layout

block content
  body
    form#<%=model.singular%>-form.form-horizontal.col-md-offset-2.col-md-10(action="<%=model.route%>/" + <%=model.singular%>._id + "/edit", method="POST")
      fieldset
        legend Edit <%=model.singularCapitalized%>

        != form

      .form-group.col-md-offset-2.col-md-10
          a.btn.btn-warning(href="<%=model.route%>") Back
          | &nbsp;
          button.btn.btn-primary(type="submit") Save
